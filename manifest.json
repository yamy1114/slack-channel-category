{
  "name": "Categorize Channel",
  "version": "1.0",
  "description": "enable to categorize your slack channels",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.slack.com/*"
      ],
      "js": [
        "./node_modules/jquery/dist/jquery.min.js",
        "./js/content.js"
      ],
      "css": [
        "./css/google_material_icon.css",
        "./css/content.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "./node_modules/crx-hotreload/hot-reload.js"
    ]
  }
}
